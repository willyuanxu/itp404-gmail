
<h5>Starred</h5>
{{#each model as |email|}}
  {{#if email.starred}}
    <EmailDisplay 
      data-test="email-starred" 
      @email={{email}} 
      @star={{action "star"}}
    />
  {{else}}
  {{/if}}
{{/each}}
<h5>Others</h5>
{{#each model as |email|}}
  {{#if email.starred}}

  {{else}}
    <EmailDisplay 
      data-test="email-unstarred" 
      @email={{email}} 
      @star={{action "star"}}
    />
  {{/if}}
{{/each}}

{{outlet}}